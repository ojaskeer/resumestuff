  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:verbose": "react-scripts test --watchAll=false --verbose --testLocationInResults",
    "test:coverage": "react-scripts test --watchAll=false --coverage",
    "test:json": "react-scripts test --watchAll=false --json --outputFile=reports/jest-results.json",
    "test:junit": "react-scripts test --watchAll=false --reporters=default --reporters=jest-junit",
    "test:ci": "CI=true react-scripts test --watchAll=false --reporters=default --reporters=jest-junit --coverage --testLocationInResults"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
